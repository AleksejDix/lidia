{
  "name": "lidia",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "build:focus": "npm run build --workspace=@aleksejdix/focus",
    "build:accordion": "npm run build --workspace=@aleksejdix/accordion",
    "build:disclosure": "npm run build --workspace=@aleksejdix/disclosure",
    "build:button": "npm run build --workspace=@aleksejdix/button",
    "build:pagination": "npm run build --workspace=@aleksejdix/pagination",
    "build:tabs": "npm run build --workspace=@aleksejdix/tabs"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@aleksejdix/accordion": "^0.1.0",
    "@aleksejdix/button": "^0.2.0",
    "@aleksejdix/checkbox": "^0.1.0",
    "@aleksejdix/datastructures": "^0.1.0",
    "@aleksejdix/disclosure": "^0.1.0",
    "@aleksejdix/feed": "^0.1.0",
    "@aleksejdix/focus": "^0.1.0",
    "@aleksejdix/modal": "^0.1.0",
    "@aleksejdix/pagination": "^0.1.0",
    "@aleksejdix/switch": "^0.1.0",
    "@aleksejdix/tabs": "^0.1.0",
    "@aleksejdix/tooltip": "^0.1.0",
    "@vueuse/core": "^9.13.0",
    "pinia": "^2.0.32",
    "uuid": "^9.0.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue-router-mock": "^0.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/vue": "^6.4.2",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@types/uuid": "^9.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/components": "^9.13.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "portal-vue": "^3.0.0-beta.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "rollup-plugin-visualizer": "^5.9.0",
    "tailwindcss": "^3.2.7",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.2.0"
  }
}
